		<emulator_link id="sega_system_16" type="single">&emulator_sega_system_16;</emulator_link>
		<emulator_link id="system_16_sound_player" type="sound">&emulator_system_16_sound_player;</emulator_link>
		<emulator_link id="tnzsemu" type="single">&emulator_tnzsemu;</emulator_link>
	</emulator_links>

	<tool_links>
